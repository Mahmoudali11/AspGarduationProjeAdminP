 @model IQueryable<WebApplication7.Models.EmployeeVM>
<a asp-action="Create" asp-controller="Employee" class="btn btn-primary">Add Employee</a>
<table class="table table-striped table-hover center-align">
    <tr>


        <th>EmployeeId</th>
        <th>EmployeeName</th>

        <th>EmployeeEmail</th>

        <th>EmployeeAddress</th>
        <th>DepartmentId</th>
        <th>EmployeeSalary</th>

        <th>Employeedat</th>

    </tr>
    @foreach (var m in Model)
    {
<tr>



    <td>@m.Id</td>
    <td>@m.Name</td>
    <td>@m.HireDate</td>
    <td>@m.Address</td>
    <td>@m.DepId</td>
    <td>@m.Salary</td>

    <td>@m.IsActive</td>

</tr>


    }
</table>
